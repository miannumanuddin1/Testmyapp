    <gap:splash src="res/screen/ios/Default-736h.png" width="1242" height="2208"/>

    <gap:splash gap:platform="winphone" src="res/screen/windows-phone/screen-portrait.jpg" />

    <feature name="DatePicker">
        <param name="ios-package" value="DatePicker" />
    </feature>

    <feature name="DatePickerPlugin">
        <param name="android-package" value="com.sharinglabs.cordova.plugin.datepicker.DatePickerPlugin"/>
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="cordova-plugin-camera" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.CameraLauncher"/>
    </feature>


    <gap:plugin name="com.sharinglabs.cordova.plugin.datepicker" />
	<gap:plugin name="nl.x-services.plugins.calendar" version="4.2.4" />

